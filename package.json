{
  "name": "decentraland-gatsby",
  "version": "0.0.0-development",
  "description": "Common Gastby components",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && tsc -p . -outDir dist --sourceMap false --skipLibCheck && postcss --base src --dir dist 'src/**/*.css'",
    "start": "npm run storybook",
    "storybook": "start-storybook -s static -p 6006",
    "semantic-release": "semantic-release",
    "build-storybook": "build-storybook && cp ./static/* ./storybook-static && mv ./storybook-static ./public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decentraland/decentraland-growth-ui.git"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss|gif|ttf|eot|svg)$": "<rootDir>/src/__mocks__/files.ts"
    }
  },
  "author": "",
  "license": "MIT",
  "release": {
    "branch": "master",
    "ci": false
  },
  "bugs": {
    "url": "https://github.com/decentraland/decentraland-growth-ui/issues"
  },
  "homepage": "https://github.com/decentraland/decentraland-growth-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-centered": "^5.3.14",
    "@storybook/addon-info": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addon-storysource": "^5.3.14",
    "@storybook/addon-viewport": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@types/flat": "0.0.28",
    "@types/jest": "^25.1.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/node": "^13.7.7",
    "@types/react-dom": "^16.9.5",
    "@types/react-highlight": "^0.12.2",
    "@types/segment-analytics": "0.0.32",
    "babel-loader": "^8.0.6",
    "flat": "^5.0.0",
    "jest": "^25.1.0",
    "postcss": "^7.0.27",
    "postcss-svg": "^3.0.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "autoprefix": "^1.0.1",
    "cssnano": "^4.1.10",
    "dcl-crypto": "^2.0.0",
    "decentraland-commons": "^5.1.0",
    "decentraland-dapps": "^7.3.0",
    "decentraland-ui": "^2.16.1",
    "framer-motion": "^1.9.0",
    "gatsby": "^2.19.24",
    "gatsby-plugin-intl": "^0.3.3",
    "highlight.js": "^9.18.1",
    "immutable": "^4.0.0-rc.12",
    "isomorphic-fetch": "^2.2.1",
    "postcss-cli": "^7.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-highlight": "^0.12.0",
    "react-markdown": "^4.3.1",
    "semantic-release": "^17.0.4",
    "web3x": "^4.0.6"
  }
}
